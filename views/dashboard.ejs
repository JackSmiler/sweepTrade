                                                const referralCode = document.getElementById("myInput").value;
                                                const fullUrl = `<%= protocol + '://' + host + '/signup?ref=' %>` + referralCode;

                                                navigator.clipboard.writeText(fullUrl);

                                                const tooltip = document.getElementById("toolbut");
                                                tooltip.innerHTML = "Copied";
